{
  "extends": "./tsconfig.app.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@ui": ["../packages/ui/src"],
      "@ui/*": ["../packages/ui/src/*"],
      "@aiprimary/question": ["../packages/question/src"],
      "@aiprimary/question/*": ["../packages/question/src/*"],
      "@question": ["../packages/question/src"],
      "@question/*": ["../packages/question/src/*"],
      "@/*": ["./src/*"],
      "@/components/*": ["./src/components/*"],
      "@/lib/*": ["./src/lib/*"],
      "@/hooks/*": ["./src/hooks/*"],
      "@/types/*": ["./src/types/*"],
      "@/api/*": ["./src/api/*"],
      "@/context/*": ["./src/context/*"],
      "@/pages/*": ["./src/pages/*"],
      "@/features/*": ["./src/features/*"]
    }
  }
}
