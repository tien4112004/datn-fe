{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "src/app/main.tsx",
    "src/app/router.tsx",
    "src/features/**/pages/**/*.tsx",
    "src/features/**/pages/**/*.ts"
  ],
  "project": ["src/**/*.{ts,tsx}"],
  "ignore": ["**/*.test.{ts,tsx}", "**/*.spec.{ts,tsx}", "**/test/**", "**/__tests__/**"],
  "ignoreDependencies": [],
  "ignoreExportsUsedInFile": true
}
